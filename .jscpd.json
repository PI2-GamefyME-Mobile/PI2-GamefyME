{
  "path": [
    "api",
    "gamefymobile"
  ],
  "ignore": [
    "**/build/**",
    "**/.dart_tool/**",
    "**/env/**",
    "**/ios/**/Pods/**",
    "**/ios/**/Runner.xcworkspace/**",
    "**/.git/**",
    "**/.idea/**",
    "**/.vscode/**",
    "**/node_modules/**",
    "gamefymobile/doc/**",
    "**/report/**",
    "**/report-clean/**",
    "**/report-temp/**"
  ],
  "reporters": [
    "console",
    "html"
  ],
  "languages": [
    "python",
    "dart",
    "kotlin",
    "swift",
    "java",
    "xml",
    "yaml",
    "json",
    "javascript",
    "typescript",
    "css",
    "html"
  ],
  "minLines": 6
}